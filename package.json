{
  "name": "rym2discogs",
  "version": "0.1.0",
  "description": "Bring your data from RateYourMusic to Discogs",
  "main": "lib",
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "babel src -d lib",
    "lint": "eslint src",
    "build": "run-p lint compile",
    "watch": "nodemon -q -w src -x 'npm run build'",
    "dev": "run-p clean watch",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-ava": "^3.1.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-promise": "^3.3.0",
    "mock-fs": "^3.11.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^3.0.0"
  },
  "dependencies": {
    "bluebird": "^3.4.0",
    "csv-parse": "^1.0.1",
    "disconnect": "^0.9.0",
    "joi": "^9.2.0",
    "lodash": "^4.16.4",
    "log-update": "^1.0.2",
    "winston": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/LostCrew/rym2discogs.git"
  },
  "keywords": [
    "rym",
    "discogs",
    "converter",
    "backup",
    "migrator"
  ],
  "author": "Emanuele Marchi <emanuele@lostcrew.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LostCrew/rym2discogs/issues"
  },
  "homepage": "https://github.com/LostCrew/rym2discogs#readme",
  "ava": {
    "files": [
      "tests/*.js",
      "!tests/mocks/*"
    ],
    "source": [
      "src/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit",
    "verbose": "false"
  }
}
